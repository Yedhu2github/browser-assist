{
  "name": "Duck Extension",
  "description": "AI Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html",
    "default_icon": "duck.png"
  },
  "permissions":["tabs","activeTab","scripting"],
  "background": {
    "service_worker": "script.js"
  },
  "content_scripts":[{
    "matches":["http://*/*", "https://*/*"],
    "js":["content.js"]
  }]
}