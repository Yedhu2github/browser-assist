{
  "name": "Duck Extension",
  "description": "AI Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "duck.png"
  },
  "permissions":["tabs","activeTab","scripting","sidePanel"],
  "background": {
    "service_worker": "script.js"
  },
  "content_scripts":[{
    "matches":["http://*/*", "https://*/*"],
    "js":["content.js"],
    "css":["content.css"]
  }],
    "side_panel": {
    "default_path": "sidepanel.html"
  }
}